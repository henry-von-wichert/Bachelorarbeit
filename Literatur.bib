@article{Daria_2018,
author = {Daria, Smazna and Sindu, Shree and Mathias, Hoppe and Luka, Hansen and Janik, Marx and Jannes, Dittman and Zaho, Kareh and Bodo, Fiedler and Holger, Kersten and Rainer, Adelung},
title = {Surface modification of highly porous 3D networks via atmospheric plasma treatment},
journal = {Contributions to Plasma Physics},
volume = {58},
number = {5},
pages = {384-393},
keywords = {3D structures, atmospheric plasma, Raman spectroscopy, surface modification},
doi = {10.1002/ctpp.201700120},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ctpp.201700120},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/ctpp.201700120},
abstract = {Tetrapodal ZnO (T-ZnO) and aerographite networks represent two highly open porous (94.6 and 99.96\%, respectively) model materials on which the impact of an atmospheric-pressure plasma was studied for the first time. The air plasma treatment by a surface barrier discharge caused remarkable surface modifications on the T-ZnO template, leading to a large number of oxygen vacancies. These observations were made using scanning electron microscopy (SEM) and Raman spectroscopy. In the second proposed set-up, pellets of aerographite material were processed to a plasma jet with pressurized air and nitrogen. Hexamethyldisiloxane (HMDSO) was introduced as a precursor into the effluent jet for nanoparticle deposition. Opposing trends in atomic concentrations versus distance to the plasma source were observed when the gas (nitrogen or pressurized air) was changed. More pronounced nanoparticle coverage occurred in the pressurized air plasma. Their distribution was studied using SEM, energy-dispersive X-ray (EDX), and Raman spectroscopy. Because of the higher oxidation in the pressurized air plasma treatment, the deposited nanostructures appeared to be a mixture of Si and SiOx nanoparticles.},
year = {2018}
}



