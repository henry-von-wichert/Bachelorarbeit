@article{baileyColloidThrustersNew1973,
  title = {Colloid Thrusters \textendash{} a New Space Propulsion System},
  author = {Bailey, A.G.},
  year = {1973},
  journal = {IEEE Electron. Power},
  volume = {19},
  pages = {281--283},
  doi = {10.1049/ep.1973.0349},
  abstract = {The duration of long-term space missions could be significantly reduced by using electric thrusters instead of chemical rockets. The colloid thruster, which gets its thrust from the electrostatic acceleration of charged liquid droplets, is potentially a very attractive propulsion system, and space tests on this type of propulsion unit could be carried out in the near future.}
}

@article{baileyInvestigationSingleSpraying1973,
  title = {Investigation of a Single Spraying Site of a Colloid Thruster},
  author = {Bailey, Adrian G.},
  year = {1973},
  journal = {J. Phys. D: Appl. Phys.},
  volume = {6},
  pages = {276--288},
  doi = {10.1088/0022-3727/6/2/313},
  abstract = {A repetitive time-of-flight system is used to study a colloid thruster spraying site which is isolated on the tip of a sharpened wire. The mean specific charge of sprayed droplets does not seem to be related to wire sharpness, but is strongly dependent on the propellant mass flowrate to the site. Spraying site characteristics are determined as a function of propellant mass flowrate (in a novel way) and of propellant temperature. The dependence of mean specific charge on mass flowrate is found to be {$<$}q/m{$>$}?m-0?57, in excellent agreement with the findings of Pfeifer and Hendricks (1968). The mean specific charge is found to be a weak function of propellant temperature, and this result may have important implications in the control of capillary-fed colloid thrusters.}
}

@article{bainesResistanceMotionSmall1965,
