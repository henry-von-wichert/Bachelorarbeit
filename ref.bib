  doi = {10.1002/phbl.19680241001},
  abstract = {Diese Rede hielt der Autor am 9. Juli 1968 in Luxemburg anl\"a\ss lich der feierlichen Er\"offnungssitzung des 4. Kongresses 1968 der Europ\"aischen Gemeinschaft f\"ur Kohle und Stahl. Wir haben die Rede hier unter Fortlassung der Anrede und sparsamer Anpassung an die Form einer Ver\"offentlichung im Druck mit freundlichem Einverst\"andnis des Autors und der Europ\"aischen Gemeinschaften, Bonn, wiedergegeben. Es lag uns ein "nicht revidierter Text" vor, und wir meinen, da\ss{} das gut ist, sp\"urt man doch deutlich die Spannung des Vortrages.},
  keywords = {Ethics}
}

@article{friichtenichtTwoMillionVoltElectrostaticAccelerator1962,
  title = {Two-{{Million-Volt Electrostatic Accelerator}} for {{Hypervelocity Research}}},
  author = {Friichtenicht, J.F.},
  year = {1962},
  journal = {Rev. Sci. Instrum.},
  volume = {33},
  pages = {209--212},
  doi = {10.1063/1.1746548},
  abstract = {A 2 000 000-v Van de Graaff positive ion accelerator has been modified to accelerate micron-sized charged particles for studies of high speed impact and for micrometeor simulation. Modification was accomplished by replacing the ion source by a particle charging and injection system. The particles are contact charged to values resulting in electric field strengths at the surface of the particle of about 2.5 x 10\^9 v/m. Using iron spheres, final velocities up to 14 km/sec have been observed. Techniques for measurement of particle parameters have been developed and measurements of the focusing properties of the modified accelerator are discussed.}
}

