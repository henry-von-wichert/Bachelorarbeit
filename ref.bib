  title = {Dust Acoustic Waves in a Magnetized Anodic Plasma},
  booktitle = {{{AIP Conf}}. {{Proc}}.},
  author = {Trottenberg, Thomas and Block, Dietmar and Piel, Alexander},
  year = {2005},
