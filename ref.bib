@article{lidskyHighlyIonizedHollow1962,
  title = {Highly {{Ionized Hollow Cathode Discharge}}},
  author = {Lidsky, L. M. and Rothleder, S. D. and Rose, D. J. and Yoshikawa, S. and Michelson, C. and Mackin, R. J.},
  year = {1962},
  journal = {J. Appl. Phys.},
  volume = {33},
  pages = {2490},
  doi = {10.1063/1.1729002},
  abstract = {A hollow cathodedischarge (HCD) is described that produces a highly ionized steady-state plasma (ne \textbackslash approx 10\^13\textendash 10\^14/cm3) at a temperature 1\textendash 10 eV, in a volume as large as 104 cm3, with background neutral gas density \textbackslash approx 10\^13/cm3. The HCD is generated by the prescription: gas flow (H2, He, A, or N2) 0.05\textendash 2 cc STP/sec through a refractory metal hollow cathode tube into a vacuum; any anode; 20\textendash 200 V dc applied. An axial induction 100\textendash 1000 G is used to collimate the discharge and to aid in starting by rf excitation. The HCD runs from the cathode interior, deep enough that p\_0 d \textbackslash approx 1 cm\texttimes mm Hg. Current range is 2.0\textendash 300 A. Various electrode configurations and a wide range of operating parameters have been studied. The external plasma density and temperature were measured by Langmuir probes. A discussion is given of the confinement mechanism and of the energy balance, both in the external plasma and in the region of the cathode itself.},
  keywords = {hollow cathode}
}

@article{liebermanModelPlasmaImmersion1989,
  title = {Model of Plasma Immersion Ion Implantation},
