@article{sigmundPreferentialSputteringIsotopic1987,
  title = {Preferential Sputtering from Isotopic Mixtures and Alloys of Near-Neighbor Elements},
  author = {Sigmund, P.},
  year = {1987},
  journal = {Nucl. Instr. and Meth. B},
  volume = {18},
  pages = {375--387},
  doi = {10.1016/S0168-583X(86)80062-3},
  abstract = {Isotope enrichment of sputtered-particle fluxes is analyzed theoretically on the basis of well-established cross sections for binary elastic collisions. An enrichment factor at low fluences of (M/Mj)\^2m, with Mj the isotopic mass and M its mean, as found previously, is reconfirmed, and its range of validity extended beyond the asymptotic limit of high ion energy. The results are discussed in relation to existing theoretical treatments and experimental data; except at very low bombarding ion energies, the agreement with experiment, is very satisfactory. Integral equations governing atomic collision cascades for polyatomic media are shown to have very simple solutions in case of small relative differences in mass and charge number. Conceptual and practical difficulties are discussed of a sputter emission model in which only atoms from the uppermost surface layer contribute to the sputter flux. It is suggested that large isotope enrichment effects observed at low ion energy are caused by recoil implantation.}
}

@article{sigmundRecollectionsFiftyYears2012,
  title = {Recollections of Fifty Years with Sputtering},
  author = {Sigmund, Peter},
  year = {2012},
  journal = {Thin Solid Films},
  volume = {520},
  pages = {6031--6049},
  doi = {10.1016/j.tsf.2012.06.003},
  abstract = {The paper is an attempt to trace milestones in the understanding of sputtering by ion bombardment. The field developed rapidly between 1955 and the end of the 1980s, but several important aspects are still under active investigation today. The focus here is on the evolution of the present understanding of the process of sputter emission. The presentation is based primarily on physical arguments, supported by results from theory referred to in the text. Many research groups have contributed to this field. Doing justice to them all appears close to impossible within the given frame.},
  keywords = {Sputter emission,Sputter erosion,Sputtering}
}

@article{sigmundSixDecadesAtomic,
  title = {Six Decades of Atomic Collisions in Solids},
