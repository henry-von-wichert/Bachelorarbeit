@inproceedings{trottenbergExperimentalInvestigationMomentum2013,
  title = {Experimental {{Investigation}} of {{Momentum Transfer}} to {{Solid Surfaces}} by the {{Impact}} of {{Energetic Ions}} and {{Atoms}}},
