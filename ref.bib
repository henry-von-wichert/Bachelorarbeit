  keywords = {Energy flux,Glow discharge,Microplasma,Plasma surface interaction,Scanning electron microscopy,Thin film modification,Transmission electron microscopy},
  file = {/Users/henry/Zotero/storage/Z4JYG9FY/Hansen et al. - 2023 - Correlations between energy flux and thin film mod.pdf}
}

@article{hansenStepEdgeSputtering2004,
