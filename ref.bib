@article{trottenbergNonElectrostaticDiagnosticsIon2012,
  title = {Non-{{Electrostatic Diagnostics}} for {{Ion Beams}}},
  author = {Trottenberg, T. and Spethmann, A. and Schneider, V. and Stahl, M. and Giesenhagen, M. and Kersten, H.},
