@article{rosenfeldtUsePassiveThermal2021,
  title = {The {{Use}} of {{Passive Thermal Probes}} for the {{Determination}} of {{Energy Fluxes}} in {{Atmospheric Pressure Plasmas}}},
  author = {Rosenfeldt, Lukas and Hansen, Luka and Kersten, Holger},
  year = {2021},
  month = nov,
  journal = {IEEE Transactions on Plasma Science},
  volume = {49},
  number = {11},
  pages = {3325--3335},
  issn = {1939-9375},
  doi = {10.1109/TPS.2021.3092752},
  abstract = {Atmospheric pressure plasmas receive great interest due to their industrial relevance based on lower cost compared with vacuum systems. To optimize these applications and possibly improve them, one has to understand plasma\textendash surface interaction. This study will emphasize the energy flux as a key parameter in plasma\textendash surface interaction. The technique of passive thermal probes (PTPs) as a suitable diagnostic to measure the energy flux is described and discussed. With such probes, the heating and cooling of a ``substrate dummy'' with a well-defined surface is measured. From the obtained temperature as a function of time, the integral energy flux can be deduced. This integral energy flux consists of various contributions, e.g., chemical contributions such as recombination or oxidation and kinetic energy of plasma species such as impinging ions, electrons, and neutrals. Beginning with a historical overview, this work aims to present the technique of the PTP in its entirety. This includes an explanation of the design along with different calibration and evaluation methods. Exemplary experimental results will be provided to emphasize its versatility and significance as diagnostic for modern atmospheric pressure plasma research.},
  keywords = {Atmospheric pressure plasmas,Atmospheric-pressure plasmas,calorimetric probe,plasma diagnostics,Plasma diagnostics,Probes},
  file = {/Users/henry/Zotero/storage/SRNG6SUL/9481343.html}
}

@article{roumeliotisHydrodynamicDescriptionElectron1989,
  title = {A Hydrodynamic Description of Electron Flow in a Strong Electric Field},
  author = {Roumeliotis, G. and Cram, L. E.},
  year = {1989},
  journal = {J. Appl. Phys. D: Appl. Phys.},
  volume = {22},
  pages = {113--119},
  doi = {10.1088/0022-3727/22/1/016},
  abstract = {The authors derive a system of equations describing the space-time development of the density, velocity and energy of a stream of electrons flowing through a background medium in the presence of a strong electric field. The equations, which resemble the equations of normal compressible fluid dynamics, provide a convenient description of electron dynamics in situations where the space-time evolution is fast enough to lead to significant non-equilibrium effects.}
}

