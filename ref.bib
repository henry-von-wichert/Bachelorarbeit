  author = {Trottenberg, Thomas and Rutscher, Jakob and Kersten, Holger},
  year = {2013},
  pages = {IEPC-2013-329},
  abstract = {This paper presents an instrument for the direct experimental determination of (one component of) the momentum that is transferred to a target at the interaction with energetic atoms or ions. Special attention is paid to the case of kinetic energies above the sputtering threshold. The importance of momentum transfer at gas\textendash surface interactions for space applications is highlighted, and a direct measurement of atmospheric drag coefficients by means of a force probe is suggested.}
}

@inproceedings{trottenbergExperimentsContactCharging2009,
  title = {Experiments on {{Contact Charging}} for an {{Electrostatic Microparticle Thruster}}},
  booktitle = {31st {{International Electric Propulsion Conference}}, {{Ann-Arbor}}, {{Michigan}}, {{USA}}},
  author = {Trottenberg, Thomas and Schneider, Viktor and Kersten, Holger},
  year = {2009},
  pages = {IEPC-2009-010},
  abstract = {This paper reports on first experiments aiming at an efficient technique for contact charging of microparticles, which could be applied for space propulsion. Fine electrode structures create high electric field strengths at the particle surface when a conductive microparticle comes in contact with the high voltage electrode. Besides propulsion of space vehicles, terrestrial applications for surface treatment and as a new particle source for hypervelocity experiments like simulation of micrometeorites appear possible.}
}

@inproceedings{trottenbergFeasibilityElectrostaticMicroparticle2007,
  title = {Feasibility of {{Electrostatic Microparticle Thrusters}}},
  booktitle = {30th {{International Electric Propulsion Conference}}, {{Florence}}, {{Italy}}},
  author = {Trottenberg, Thomas and Kersten, Holger and Neumann, Horst},
  year = {2007},
  pages = {IEPC-2007-179},
  abstract = {The paper discusses the feasibility of electrostatic propulsion which uses microparticles as propellant. Two novel thruster concepts are proposed which differ both from electrospray-like colloid thrusters and nano field-emission thrusters, which extract nanoparticles from suspensions. It is shown that particle charging in a plasma is not sufficient for electrostatic acceleration. Moreover, it appears technically difficult to extract charged particles out of a plasma for subsequent acceleration without being discharged. It is proposed to charge particles with low secondary electron emission using an energetic electron beam, e.g. with graphite particles surface potentials of \$-90\$ V can be obtained. Another promising concept is charging by contact with needle electrodes at high electrostatic potential (\$\textbackslash sim\$ 20 kV), which allows for maximum possible specific charges.}
}

@article{trottenbergFeasibilityElectrostaticMicroparticle2008,
  title = {Feasibility of Electrostatic Microparticle Propulsion},
  author = {Trottenberg, Th. and Kersten, H. and Neumann, H.},
  year = {2008},
  journal = {New J. Phys.},
  volume = {10},
  pages = {063012},
  doi = {10.1088/1367-2630/10/6/063012},
  abstract = {This paper discusses the feasibility of electrostatic space propulsion which uses microparticles as propellant. It is shown that particle charging in a plasma is not sufficient for electrostatic acceleration. Moreover, it appears technically difficult to extract charged particles out of a plasma for subsequent acceleration without them being discharged. Two novel thruster concepts are proposed. In the first one, particles with low secondary electron emission are charged using energetic electrons in the order of magnitude of 100 eV. The second concept charges the particles by contact with needle electrodes at high electrostatic potential ( 20 kV). Both methods allow the maximum possible charges on microparticles.}
}

@article{trottenbergInflightPlasmaDiagnostic2021,
  title = {An In-Flight Plasma Diagnostic Package for Spacecraft with Electric Propulsion},
  author = {Trottenberg, T. and Bansemer, F. and B{\"o}ttcher, S. and Feili, D. and Henkel, H. and Hesse, M. and Kersten, H. and Kr{\"u}ger, T. and Laube, J. and Lazurenko, A. and Sailer, D. and Schuster, B. and Seimetz, L. and Spethmann, A. and Weis, S. and {Wimmer-Schweingruber}, R. F.},
  year = {2021},
  journal = {EPJ Techn. Instrum.},
  volume = {8},
  pages = {16},
  doi = {10.1140/epjti/s40485-021-00073-8},
  abstract = {The plasma diagnostics presented in this article target the plasma surrounding a spacecraft that is created by the electric thruster and its surface modifying effects. The diagnostic package includes a retarding potential analyzer, a plane Langmuir probe, and an erosion sensor. The paper describes the instrument as well as suitable test environments for mimicking the effects expected in space and shows test results. The system is to fly for the first time on the Heinrich Hertz satellite, which is scheduled to be launched in 2023. The spacecraft will be equipped with a pair of Highly Efficient Multistage Plasma Thrusters (HEMPT) and a pair of Hall thrusters for redundancy.},
  keywords = {Charge-exchange collisions,Electric Propulsion,Electric spacecraft propulsion,Ion beam,Ion thrusters,Plasma diagnostics,RPA,Sputtering}
}

@inproceedings{trottenbergInterferometricForceProbe2015,
  title = {An {{Interferometric Force Probe}} for {{Thruster Plume Diagnostics}}},
  booktitle = {34th {{International Electric Propulsion Conference}}, {{Kobe}}, {{Japan}}},
  author = {Trottenberg, Thomas and Spethmann, Alexander and Kersten, Holger},
  year = {2015},
  pages = {IEPC-2015-419},
  abstract = {In this article, we present a novel probe for the measurement of forces exerted by a beam of ions and neutral atoms on a small test surface. The probe is intended for thruster plume diagnostics, in particular for the determination of the spatial distribution of the momentum in an exhaust beam. The instrument makes use of a cantilever whose deflection is measured interferometrically along two axes, which enables a simultaneous measurement of two independent components of the force vector. This contribution focuses on setup and calibration of the probe. An example measurement in an ion beam is presented together with a comparison measurement done with an earlier developed force probe based on a different functional principle.}
}

@article{trottenbergInterferometricForceProbe2018,
  title = {An Interferometric Force Probe for Beam Diagnostics and the Study of Sputtering},
  author = {Trottenberg, Thomas and Spethmann, Alexander and Kersten, Holger},
  year = {2018},
  journal = {EPJ Techn. Instrum.},
  volume = {5},
  pages = {3},
  doi = {10.1140/epjti/s40485-018-0044-2},
  abstract = {A novel probe for the measurement of forces exerted by a beam of ions and neutral atoms on a small target is described. The force probe is intended for various applications: First, diagnostics of thruster plumes of electric spacecraft propulsion engines, in particular the determination of the spatial distribution of the momentum in an exhaust beam, and second, the study of sputtering of solid targets by means of the related momentum transfer. The instrument makes use of a sensitive cantilever whose elastic deflection is measured interferometrically along two axes and enables that way a simultaneous measurement of two independent components of the force vector. The present contribution focuses on setup and calibration of the probe. Example measurements with obliquely irradiating ion beams are performed.}
}

