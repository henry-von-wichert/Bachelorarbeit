  abstract = {The article reports on non-electrostatic diagnostics for ion beams. A novel galvanometric force measuring probe is presented, and results are compared with corresponding measurements by a calorimetric technique and using falling microparticles. The target chamber is operated at argon pressures where charge-exchange collisions are important. It is highlighted that non-electrostatic methods are sensitive for ions as well as energetic neutral atoms. For the first time, bombardment induced light emission (BLE) from solid targets is proposed as simple beam diagnostic},
  keywords = {bombardment induced light emission,calorimetric probe,charge-exchange collisions,energetic neutral atoms,force probe,Ion beam,microparticle}
}

@article{trottenbergNonelectrostaticDiagnosticsIon2012,
  title = {Non-Electrostatic Diagnostics for Ion Beams and Sputter Effects},
  author = {Trottenberg, T. and Spethmann, A. and Rutscher, J. and Kersten, H.},
  year = {2012},
  journal = {Plasma Phys. Control. Fusion},
  volume = {54},
  pages = {124005},
  doi = {10.1088/0741-3335/54/12/124005},
  abstract = {This paper reports on novel non-electrostatic diagnostics for ion beams. A force measuring probe for determination of the momentum flux in a particle beam is presented, and results are compared with corresponding measurements by a calorimetric technique. The target chamber is operated in a pressure regime where charge-exchange collisions are important. It is highlighted that non-electrostatic methods are sensitive for ions as well as energetic neutral atoms. Further, a technique is demonstrated which allows a measurement of the momentum associated with sputtered target atoms and recoil beam particles.}
}

