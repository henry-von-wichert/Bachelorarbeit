@article{pilchDynamicsSmallDust2007a,
  title = {Dynamics of Small Dust Clouds Trapped in a Magnetized Anodic Plasma},
  author = {Pilch, Iris and Piel, Alexander and Trottenberg, Thomas},
  year = {2007},
  journal = {Phys. Plasmas},
  volume = {14},
  pages = {123704},
  doi = {10.1063/1.2819315},
  abstract = {Small dust clouds, which are confined in an anodic plasma, are studied with respect to their structure and their response to modulation of the anode bias. The dust cloud is displaced from the center of the discharge by a process similar to the void mechanism in radio-frequency discharges under microgravity. The top layers of the dust cloud are in a crystalline state and the cloud performs a slow rotation about the magnetic field direction. For modulation frequencies below 15 Hz, a sloshing and stretching motion in the confining potential well is found. Spontaneously excited dust density waves are observed when the dust cloud exceeds a minimum size. The waves are characterized by sickle-shaped wave fronts. No standing waves were found. The wave dispersion shows an influence of the boundedness of the system in terms of a frequency cutoff.}
}

@article{pilchTorusshapedDustClouds2008a,
  title = {Torus-Shaped Dust Clouds Trapped in a Magnetized Anodic Plasma},
  author = {Pilch, Iris and Reichstein, Torben and Piel, Alexander},
  year = {2008},
  journal = {Phys. Plasmas},
  volume = {15},
  pages = {103706},
  doi = {10.1063/1.3006085},
  abstract = {Dust particles confined in a magnetized anodic plasma can form a torus-shaped cloud with a dust-free region  void  in the center. Most of the dust particles perform a rotational motion about the major axis of the torus. The torus-shaped dust cloud and the velocity of the particles are studied by varying the external plasma parameters like magnetic field strength and rf-power of the source plasma. Two-dimensional potential contours are measured with an emissive probe. The results are used to discuss the force balance between electric field force and ion drag acting on the dust particles that determines the void size.}
}

@article{pillowCriticalReviewSpectral1981a,
  title = {A Critical Review of Spectral and Related Physical Properties of the Hollow Cathode Discharge},
  author = {Pillow, M. E.},
  year = {1981},
  journal = {Spectrochimica Acta, Part B: Atomic Spectroscopy},
  volume = {36},
  pages = {821--843},
  doi = {10.1016/0584-8547(81)80064-X},
  abstract = {Investigations made over the last fifty years, of the electrical properties which differentiate a hollow cathode d.c. discharge from one before a plane cathode, have been summarized. The wide range of dimensions and pressure conditions used by different workers have been considered, and related where possible to results, to give a more unified picture. Data are collected on energies and densities of electrons, and of positive ions, in hollow cathode cavities, with a view to establishing the principles governing spectral line emission. Results of measurements on spectral line intensities, of carrier gases and of atoms sputtered from the cathode, are discussed in relation to the activity, already considered, of electrons and positive ions. The gaps and contradictions in present knowledge of conditions for useful light emission are indicated.},
  keywords = {hollow cathode}
}

