@article{hansenConventionalNonconventionalDiagnostics2022,
  title = {Conventional and Non-Conventional Diagnostics of a Stable Atmospheric Pressure {{DC}} Normal Glow Microplasma Discharge Intended for in Situ {{TEM}} Studies},
  author = {Hansen, Luka and Kohlmann, Niklas and Sch{\"u}rmann, Ulrich and Kienle, Lorenz and Kersten, Holger},
  year = {2022},
  month = mar,
  journal = {Plasma Sources Science and Technology},
  volume = {31},
  number = {3},
  pages = {035013},
  publisher = {{IOP Publishing}},
  issn = {0963-0252},
  doi = {10.1088/1361-6595/ac5101},
  abstract = {A simple setup utilizing parallel flat electrodes with a 50\textendash 150 {$\mu$}m interelectrode distance divided by a Kapton spacer with a 1 mm diameter whole as discharge region intended for in situ transmission electron microscope studies is presented. The rather small setup operated in Ar or He results in an atmospheric pressure DC normal glow discharge and is investigated using various diagnostics. I\textendash V characteristics show a glow-like behavior of the microplasma. Significant differences due to the working gas, electrode material and electrode distance have been observed. Currents in the range of 0.5\textendash 3 mA resulted in electrode potentials of 140\textendash 190 V for most experimental conditions. Optical emission spectroscopy and imaging revealed stable plasma operation and enabled the determination of current densities (approx. 16 mA mm-2 for He, or 28 mA mm-2 for Ar) independent of the input current as the discharge channel grows in diameter. Sheath thicknesses in the range of a few {$\mu$}m have been calculated by the collision-dominated Child\textendash Langmuir law and trends are confirmed by the optical imaging. Energy flux measurements revealed a pronounced effect of ions on the measurement process and resulted in high energy fluxes locally up to 275 W cm-2. Effective secondary electron emission coefficients ranging from 1 to 1.6 depending on the discharge conditions have been determined based on the energy balance at the cathode.},
  langid = {english},
  file = {/Users/henry/Zotero/storage/KDGXMVTP/Hansen et al. - 2022 - Conventional and non-conventional diagnostics of a.pdf}
}

