@article{reckEnergyFluxCharacterisation2022,
  title = {Energy {{Flux Characterisation}} of {{Atmospheric Pressure Plasma Spray Torches}} with {{Passive Thermal Probes}}},
  author = {Reck, Kristian A. and Hansen, Luka and Stummer, Maximilian and Kewitz, Thorben and Testrich, Holger and Hinterer, Andreas and Foest, R{\"u}diger and Kersten, Holger},
  year = {2022},
