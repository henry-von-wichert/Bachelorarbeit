@article{kmentRoleIonBombardment2016,
  title = {Role of Ion Bombardment, Film Thickness and Temperature of Annealing on {{PEC}} Activity of Very-Thin Film Hematite Photoanodes Deposited by Advanced Magnetron Sputtering},
  author = {Kment, Stepan and Cada, Martin and Hubicka, Zdenek and Krysa, Josef and Kmentova, Hana and Olejnicek, Jiri and Cilova, Zuzana Zlamalova and Zboril, Radek},
  year = {2016},
