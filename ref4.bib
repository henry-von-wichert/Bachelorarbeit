@article{willemsMassSpectrometryNeutrals2020,
  title = {Mass Spectrometry of Neutrals and Positive Ions in {{He}}/{{CO}} {\textsubscript{2}} Non-Equilibrium Atmospheric Plasma Jet},
  author = {Willems, Gert and Hecimovic, Ante and Sgonina, Kerstin and Carbone, Emile and Benedikt, Jan},
  year = {2020},
  month = mar,
  journal = {Plasma Physics and Controlled Fusion},
  volume = {62},
  number = {3},
  pages = {034005},
  issn = {0741-3335, 1361-6587},
  doi = {10.1088/1361-6587/ab6b4c},
  abstract = {Abstract                            Neutral species and positive ions are measured by means of mass spectrometry in the effluent of the non-equilibrium atmospheric plasma jet operated in He/0\%\textendash 1\%CO               2               gas mixture. The capacitively coupled plasma source is the predecessor of the known COST reference jet with identical performance and it is operated in a diffuse mode with gas temperature close to the room temperature. Threshold ionisation molecular beam mass spectrometry is used to measure absolute densities of CO, O               2               , O and O               3               species. The CO molecules are generated with densities up to 2~\texttimes ~10               15               cm               -3               at 1 W absorbed power. The O               2               density is {$\sim$}6 times lower than the CO density and even lower than the O density at level of 4.5~\texttimes ~10               14               cm               -3               . The O               3               density is negligibly small at 4~\texttimes ~10               12               cm               -3               . The measured O               2               , O               3               and O absolute densities add up to {$\sim$}57\% of the expected oxygen amount. The high O density could indicate that the vibrational excitation of CO               2               is ineffective in this plasma and gas mixture, because O would otherwise react quickly with vibrationally excited CO               2               (v) to form CO and O               2               , the low energy efficiency is in agreement with the possibly low vibrational excitation. The highest energy efficiency is just 5\% with 1\% conversion efficiency. Ion mass spectrometry have been used to measure positive and negative ions, where only positive ions have been detected. The ion with highest signal is the                                                                                                                                                                                                        O                                                                             2                                                                                                                 +                                                                                                        ion, probably due to its low ionisation energy.                                                                                                                                                                                                        CO                                                                             2                                                                                                                 +                                                                                                        ,                                                                                                                                                                                                        O                                                                             3                                                                                                                 +                                                                                                        ,                                                                                                                                                                                                        (                           CO                           )                                                                             2                                                                                                                 +                                                                                                        , but no CO               +               , and protonated clusters or clusters containing water molecules have been detected as well. Neutrals and positive ions have been measured as a function of CO               2               admixture, applied power and distance to the jet nozzle. The observed trends can be used for validation of plasma-chemistry models.},
  file = {W\:\\Bibliothek\\Literatur\\Literatur_MS\\Willems_2020_Plasma_Phys._Control._Fusion_62_034005.pdf}
}

@phdthesis{willemsReactionChemistryNonequilibrium2018,
  title = {Reaction Chemistry in Non-Equilibrium Atmospheric Pressure Plasmas},
  author = {Willems, Gert},
  year = {2018},
  abstract = {Nichtgleichgewichts Atmosph\"arendruck Plasmen sind eine gute Quelle f\"ur reaktive Teilchen, die \"uber verschiedene Prozessparameter leicht gesteuert werden k\"onnen. Der effluent des COST reference Jets sowie die MS variante wurden mit Molekularstrahl Massenspektrometrie analysiert. Die Plasma Jets wurden in Helium Atmosph\"are betrieben mit einer kontrollierten Beimischung von Wasser Dampf, \textbackslash (N\_\{2\}/O\_\{2\}\textbackslash ), \textbackslash (CO\_\{2\}\textbackslash ) oder \textbackslash (O\_\{2\}\textbackslash ). Erste Einblicke in die komplexe Reaktionschemie des Plasmas lieferte die Kombination von experimentellen Daten und einem 2D Achsen-symmetrischen fluid Model des Spezies Transport und der Rekombinationsreaktionen. Die Analyse der neutralen Plasma Produkte wurde erg\"anzt durch quantitative Messungen des Ionenflusses mit dem Massenspektrometer. Die in dieser Arbeit gewonnenen absoluten Dichten k\"onnen als Input in zuk\"unftigen Arbeiten benutzt werden um komplexere Probleme zu analysieren, z.B. Plasmen mit Biologischen Systemen, oder als benchmark f\"ur Plasma- Reaktionschemie Modelle.},
  keywords = {_tablet,Abschlussarbeit,APPJ,Atmosphärendruckplasma,COST,Dissertation,Hochfrequenzentladung,Hydroxyl,Massenspektrometer,Plasmachemie},
  file = {W\:\\Bibliothek\\Literatur\\Externe Abschlussarbeiten\\WillemsDissertation2018.pdf}
}

@article{williamsMeasurementKineticDust2007a,
