@article{linChargeTransferSurfaces1983a,
  title = {Charge Transfer at Surfaces: {{A}} Model for Ionization in {{SIMS}}},
  author = {Lin, J. H. and Garrison, B. J.},
  year = {1983},
  journal = {J. Vac. Sci. Technol. A},
  volume = {1},
  pages = {1205},
