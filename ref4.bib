@inproceedings{matsuiPlumeCharacterizationPlasma2006a,
  title = {Plume {{Characterization}} of {{Plasma Thrusters Using Diode-Laser Absorption Spectroscopy}}},
  booktitle = {44th {{AIAA Aerospace Sciences Meeting}} and {{Exhibit}}, {{Reno}}, {{Nevada}}, {{USA}}},
  author = {Matsui, Makoto and Yokota, Shigeru and Takayanagi, Hiroki and Koizumi, Hiroyuki and Komurasaki, Kimiya and Arakawa, Yoshihiro},
  year = {2006},
  pages = {AIAA-2006-767},
  doi = {10.2514/6.2006-767},
  abstract = {Characteristics of a magnetic layer type Hall thruster plume were diagnosed by laser absorption spectroscopy. Translational temperature and number density distributions xenon atom was deduced using an absorption line of XeI 823.16 nm. As a result, the temperature was around 430K in the almost all measured region, though it was overestimated at the acceleration channel exit due to the Zeeman effect. The maximum number density was 2.2 x 10\^19 m\^-3 at the channel exit. Then, the number density decreased by one order at 200mm away from the exit.}
}

@article{matsuokaIonExtractionMicrowave1991a,
  title = {Ion Extraction from Microwave Plasma Excited by Ordinary and Extraordinary Waves and Applications to the Sputtering Deposition},
  author = {Matsuoka, Morito and Ono, Ken'ichi},
  year = {1991},
  journal = {J. Vac. Sci. Technol. A},
  volume = {9},
  pages = {691--695},
  doi = {10.1116/1.577345},
  abstract = {The right hand circularly polarized waves propagating along the magnetic flux are the most useful for producing overdense plasma. Microwaves channeled perpendicular to the magnetic flux are also efficient for some applications, including conductive-film preparations by sputtering and chemical vapor deposition (CVD) processes. In this study, the plasma excitation by means of the ordinary and extraordinary microwaves have been investigated for conductive film deposition. These waves effectively generate plasma with an approximately cut off density in a low-gas-pressure atmosphere. Ion streams with 4 mA/cm2 current density are extracted from the plasma excited by the extraordinary microwaves under a pressure below 0.02 Pa. These waves are used for the sputtering deposition of conductive films, including iron-nitride films with giant magnetization and Mo films.}
}

@article{matyashParticleCellSimulation2007a,
  title = {Particle in {{Cell Simulation}} of {{Low Temperature Laboratory Plasmas}}},
  author = {Matyash, K. and R, Schneider and Taccogna, F. and Hatayama, A. and Longo, S. and Capitelli, M. and Tskhakaya, D. and Bronold, F. X.},
