@article{garcia-rosalesRevisedFormulaeSputtering1995a,
  title = {Revised Formulae for Sputtering Data},
  author = {{Garc{\'i}a-Rosales}, C. and Eckstein, W. and Roth, J.},
  year = {1995},
  journal = {Journal of Nuclear Materials},
  volume = {218},
  pages = {8--17},
  doi = {10.1016/0022-3115(94)00376-9},
  abstract = {A revision and improvement of analytic formulae for calculating sputtering yields is performed based on the large number of experimental and calculated sputtering yield data accumulated at IPP in the last three decades. The Bohdansky formula for calculating sputtering yields as a function of energy is revised by introducing a nuclear stopping cross-section based on an analytic fit to the Kr\textemdash C potential. New analytic expressions for the two fit parameters Q and E\_th of the Bohdansky formula are deduced. Yamamura's formula for the angular dependence of the sputtering yield is shown not to be valid for self-sputtering and for heavy projectiles at energies near the threshold.}
}

