@article{emmertElectricSheathPresheath1980a,
  title = {Electric Sheath and Presheath in a Collisionless, Finite Ion Temperature Plasma},
  author = {Emmert, G. A. and Wieland, R. M. and Mense, A. T. and Davidson, J. N.},
