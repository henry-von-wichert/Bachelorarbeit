@article{couedelResidualDustCharges2006a,
  title = {Residual Dust Charges in Discharge Afterglow},
  author = {Cou{\"e}del, L. and Mikikian, M. and Boufendi, L. and Samarian, A. A.},
  year = {2006},
  journal = {Phys. Rev. E},
  volume = {74},
  pages = {026403},
  doi = {10.1103/PhysRevE.74.026403},
  abstract = {An on-ground measurement of dust-particle residual charges in the afterglow of a dusty plasma was performed in a rf discharge. An upward thermophoretic force was used to balance the gravitational force. It was found that positively charged, negatively charged, and neutral dust particles coexisted for more than 1 min after the discharge was switched off. The mean residual charge for 200-nm-radius particles was measured. The dust particle mean charge is about -5e at a pressure of 1.2 mbar and about -3e at a pressure of 0.4 mbar.}
