@article{kechkarInvestigationAbsoluteAtomic2014,
  title = {Investigation of Absolute Atomic Fluorine Density in a Capacitively Coupled {{SF}} {\textsubscript{6}} /{{O}} {\textsubscript{2}} /{{Ar}} and {{SF}} {\textsubscript{6}} /{{Ar}} Discharge},
  author = {Kechkar, S and Babu, S K and Swift, P and Gaman, C and Daniels, S and Turner, M},
