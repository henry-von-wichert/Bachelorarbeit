@article{longmierValidatingPlasmaMomentum2009a,
  title = {Validating a {{Plasma Momentum Flux Sensor}} to an {{Inverted Pendulum Thrust Stand}}},
  author = {Longmier, Benjamin W. and Reid, Bryan M. and Gallimore, Alec D. and {Chang-D{\'i}az}, Franklin R. and Squire, Jared P. and Glover, Tim W. and Chavers, Greg and Bering III., Edgar A.},
  year = {2009},
  journal = {J. Propul. Power},
  volume = {25},
  pages = {746--752},
