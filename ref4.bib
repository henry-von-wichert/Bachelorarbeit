@article{leeAmmoniabasedPlasmaTreatment2016,
  title = {Ammonia-Based Plasma Treatment of Single-Walled Carbon Nanotube Thin Films for Bio-Immobilization},
  author = {Lee, Jaemin and Efremov, Alexander and Son, Ryeo Gang and Pack, Seung Pil and Lee, Hyun Woo and Kim, Kwangsoo and Kwon, Kwang-Ho},
  year = {2016},
  month = aug,
  journal = {Carbon},
  volume = {105},
