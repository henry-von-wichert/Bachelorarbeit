  pages = {1459},
  doi = {10.1063/1.871736},
  abstract = {Using a two-fluid model it is shown analytically that in collisional slightly ionized plasmas, i.e., for many typical low pressure discharges, at the sheath edge the drift velocity of the plasma can be smaller than the ambipolar sound speed of the ions. This basic result was known earlier by numerical calculations but it is demonstrated here using simple analytical expressions. Under these conditions the Bohm criterion, in its usual formulation, is a sufficient condition for the existence of a positive boundary sheath, but it is not a necessary condition.},
  keywords = {Bohm criterion,Plasma sheath}
}

@article{valentiniSheathFormationLowpressure2014a,
  title = {Sheath Formation in Low-Pressure Discharges, the {{Bohm}} Criterion and the Consequences of Collisions},
  author = {Valentini, H.-B. and Kaiser, D.},
  year = {2014},
  journal = {Plasma Sources Sci. Technol.},
  volume = {23},
  pages = {015004},
  doi = {10.1088/0963-0252/23/1/015004},
  abstract = {The space charge density in low-pressure discharges results from the generation of charged particles, the momentum transfer from these particles to the neutral gas and the electric field. A simplified model is used to treat this process analytically and numerically across the whole plasma. The effect of the electric field alone can cause the formation of the space charge sheath if the ion drift velocity v\_i to the wall exceeds the modified Bohm velocity v\_C = v\_B \textbackslash times (ni/ne)1/2, where v\_B is the Bohm velocity and ni and ne are the number densities of the ions and the electrons, respectively. However, a domain with v\_i \textbackslash geq v\_C can occur only if the effect of collisions is weak. This domain is very narrow and does not come up to the wall. Limits of the electric field strength determining the sheath formation are given. It is shown that the electric field strength cannot be set equal to zero at v\_i = v\_B or v\_C under collisional conditions. The sheath extends from the region near the wall towards the centre and a result of that is to lower v\_i with respect to v\_B as the collisionality rises. These results are used to take into consideration various sheath criteria. The Bohm criterion takes into account the effect of the electric field only and reveals a well-defined sheath edge at v\_i = v\_B. This criterion remains a useful approximation of the sheath edge in almost collisionless plasmas as well. Under collisional conditions the definition of the sheath edge becomes more difficult and a little arbitrary. This paper takes into account new sheath criteria modified for the case of finite collisionality. The divergence between the densities of the ions and the electrons, the gradients of the space charge density and of the generalized Bohm speed v\_C are studied as functions of v\_i or the distance from the wall. These criteria are compared with the collisionally modified Bohm criteria proposed by Godyak (1982 Phys. Lett. A 89 80), Valentini (1996 Phys. Plasmas 3 1459), Chen (1998 Phys. Plasmas 5 804) and Brinkmann},
  keywords = {Bohm criterion,collisions,Plasma sheath}
}

@article{vanderunstraatAbsoluteBolometerDetector1970a,
  title = {An Absolute Bolometer Detector for Energetic Neutral Particles},
  author = {{van de Runstraat}, C.A. and {Wijnaendts van Resandt}, R. and Los, J.},
  year = {1970},
  journal = {J. Phys. E: Sci. Instrum.},
  volume = {3},
  pages = {575--578},
  doi = {10.1088/0022-3735/3/7/434},
  abstract = {A bolometer detector for atomic beams is described. It consists of a cage, whose shape ensures nearly complete dissipation of the beam power, and a thermistor attached to it. A convenient calibration procedure with ion beams is provided by simultaneous operation as thermal detector and as Faraday cup. The smallest detectable power is \textsuperscript{-8\$} W. Absolute currents of neutral particle beams with energies down to 100 eV have been measured.}
}

@article{vandijkPlasmaModellingNumerical2009a,
  title = {Plasma Modelling and Numerical Simulation},
  author = {{van Dijk}, J. and Kroesen, G. M. W. and Bogaerts, A.},
  year = {2009},
  journal = {J. Phys. D: Appl. Phys.},
  volume = {42},
  pages = {190301},
  doi = {10.1088/0022-3727/42/19/190301},
  abstract = {Plasma modelling is an exciting subject in which virtually all physical disciplines are represented. Plasma models combine the electromagnetic, statistical and fluid dynamical theories that have their roots in the 19th century with the modern insights concerning the structure of matter that were developed throughout the 20th century. The present cluster issue consists of 20 invited contributions, which are representative of the state of the art in plasma modelling and numerical simulation. These contributions provide an in-depth discussion of the major theories and modelling and simulation strategies, and their applications to contemporary plasma-based technologies. In this editorial review, we introduce and complement those papers by providing a bird's eye perspective on plasma modelling and discussing the historical context in which it has surfaced.}
}

