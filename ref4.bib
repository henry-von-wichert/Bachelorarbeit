@article{barnesTransportDustParticles1992a,
  title = {Transport of Dust Particles in Glow-Discharge Plasmas},
  author = {Barnes, Michael S. and Keller, John H. and Forster, John C. and O{\textbackslash}char39Neill, James A. and Coultas, D. Keith},
  year = {1992},
  month = jan,
  journal = {Phys. Rev. Lett.},
  volume = {68},
  number = {3},
  pages = {313--316},
