@article{aucielloDeterminationExcitedSputtered1982a,
  title = {Determination of Excited Sputtered Atoms Kinetic Energy as a Contribution to the Understanding of the Excitation Phenomenon in the Sputtering Process},
  author = {Auciello, O.},
  year = {1982},
  journal = {Nucl. Instr. and Meth.},
  volume = {194},
  pages = {549--553},
  doi = {10.1016/0029-554X(82)90579-1},
  abstract = {The kinetic energy of excited sputtered particles from ion bombarded alkali-halides was determined as a function of the ion beam energy. The energy of the excited atoms was obtained by deconvoluting the photon intensity vs distance from the surface curves via a formalism recently developed. The observed excited atoms kinetic energy-beam energy dependence supports, together with other work, a new model which suggests that the excited atoms originate from direct (this for oblique angle of incidence of the ion beam only) or near direct recoils produced in incident ion-atom and atom-atom binary collisions at or very near the surface.}
}

