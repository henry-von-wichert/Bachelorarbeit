@article{niemiRoleHeliumMetastable2011,
  title = {The Role of Helium Metastable States in Radio-Frequency Driven Helium\textendash Oxygen Atmospheric Pressure Plasma Jets: Measurement and Numerical Simulation},
  shorttitle = {The Role of Helium Metastable States in Radio-Frequency Driven Helium\textendash Oxygen Atmospheric Pressure Plasma Jets},
  author = {Niemi, K. and Waskoenig, J. and Sadeghi, N. and Gans, T. and O'Connell, D.},
  year = {2011},
  month = aug,
  journal = {Plasma Sources Science and Technology},
  volume = {20},
  number = {5},
  pages = {055005},
  issn = {0963-0252},
  doi = {10.1088/0963-0252/20/5/055005},
  abstract = {Absolute densities of metastable He(23S1) atoms were measured line-of-sight integrated along the discharge channel of a capacitively coupled radio-frequency driven atmospheric pressure plasma jet operated in technologically relevant helium\textendash oxygen mixtures by tunable diode-laser absorption spectroscopy. The dependences of the He(23S1) density in the homogeneous-glow-like {$\alpha$}-mode plasma with oxygen admixtures up to 1\% were investigated. The results are compared with a one-dimensional numerical simulation, which includes a semi-kinetical treatment of the pronounced electron dynamics and the complex plasma chemistry (in total 20 species and 184 reactions). Very good agreement between measurement and simulation is found. The main formation mechanisms for metastable helium atoms are identified and analyzed, including their pronounced spatio-temporal dynamics. Penning ionization through helium metastables is found to be significant for plasma sustainment, while it is revealed that helium metastables are not an important energy carrying species into the jet effluent and therefore will not play a direct role in remote surface treatments.},
  langid = {english}
}

@article{ningEffectsPowderProperties2007a,
  title = {The Effects of Powder Properties on In-Flight Particle Velocity and Deposition Process during Low Pressure Cold Spray Process},
  author = {Ning, Xian-Jin and Jang, Jae-Hoon and Kim, Hyung-Jun},
  year = {2007},
  journal = {Appl. Surf. Sci.},
  volume = {253},
  pages = {7449--7455},
  doi = {10.1016/j.apsusc.2007.03.031},
  abstract = {In cold spray process, impacting velocity and critical velocity of particles dominate the deposition process and coating properties for given materials. The impacting velocity and critical velocity of particles depend on the powder properties and cold spray conditions. In the present study, the in-flight particle velocity of copper powder in low pressure cold spraying was measured using an imaging technique. The effects of particle size and particle morphology on in-flight particle velocity and deposition efficiency were investigated. The critical velocity of copper powder was estimated by combining the in-flight particle velocity and deposition efficiency. The effect of annealing of feedstock powder on deposition and critical velocity was also investigated. The results showed that the irregular shape particle presents higher in-flight velocity than the spherical shape particle under the same condition. For irregular shape particles, the in-flight velocity decreased from 390 to 282 m/s as the particle size increases from 20 to 60 mm. Critical velocities of about 425 m/s and more than 550 m/s were estimated for the feedstock copper powder with spherical and irregular shape morphology, respectively. For the irregular shape particles, the critical velocity decreased from more than 550 to 460 m/s after preheating at 390 \textdegree C for 1 h. It was also found that the larger size powder presents a lower critical velocity in this study.}
}

