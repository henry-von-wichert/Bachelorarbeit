@article{reichsteinSimulationDustStreaming2011a,
  title = {Simulation of Dust Streaming in Toroidal Traps: {{Stationary}} Flows},
  author = {Reichstein, Torben and Piel, Alexander},
  year = {2011},
  journal = {Phys. Plasmas},
  volume = {18},
  pages = {083705},
  doi = {10.1063/1.3610376},
  abstract = {Molecular-dynamic simulations were performed to study dust motion in a toroidal trap under the influence of the ion drag force driven by a Hall motion of the ions in \$E \textbackslash times B\$ direction, gravity, interparticle forces, and friction with the neutral gas. This article is focused on the inhomogeneous stationary streaming motion. Depending on the strength of friction, the spontaneous formation of a stationary shock or a spatial bifurcation into a fast flow and a slow vortex flow is observed. In the quiescent streaming region, the particle flow features a shell structure which undergoes a structural phase transition along the flow direction.}
