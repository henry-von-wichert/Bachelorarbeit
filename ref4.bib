@article{taylorLargeDoublePlasma1972a,
  title = {A {{Large Double Plasma Device}} for {{Plasma Beam}} and {{Wave Studies}}},
  author = {Taylor, R. J. and MacKenzie, K. R. and Ikezi, H.},
  year = {1972},
  journal = {Rev. Sci. Instrum.},
  volume = {43},
  pages = {1675--1678},
  doi = {10.1063/1.1685522},
  abstract = {The DP (or double plasma) device consists of two essentially collisionless plasmas that are separated from each other (for electrons only) by a negatively biased grid. Large cross section beams of ions are produced in one plasma by raising the potential of the other. Ion waves or shocks are produced in one plasma by oscillatory or large sudden (or ramp) potential changes in the other. Electrons emitted from two electrically separate spatial arrays of small filaments produce the two plasmas with densities in the range from below 108 to 1010 ion/cm3 with noise levels {$<\backslash$}delta n/n{$>$} \textbackslash approx 5 \texttimes{} 10-4.}
}

@inproceedings{telibanAdvancedDiagnosticsFull2008a,
  title = {Advanced {{Diagnostics}} for {{Full Characterization}} of {{ECR-MW Ion Sources}}},
  booktitle = {International {{Space Propulsion Conference}}, {{Heraklion}}, {{Crete}}, {{Greek}}},
  author = {Teliban, Iulian and Trottenberg, Thomas and Schneider, Viktor and Kersten, Holger},
  year = {2008},
  abstract = {The performance of an ECR-MW broad beam ion source using different diagnostics is investigated. For this purpose tools used in plasma diagnostics are improved and adapted. The ECR plasma in the source and the secondary plasma in the target chamber are analyzed using Langmuir probes. Ion current density profiles in the beam are measured using a Faraday-cup array. The energy of the beam ions is carefully investigated using a three-grid retarding-field analyzer. Correlations between axial and radial profiles of the ion beam and source plasma are investigated.},
