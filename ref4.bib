@article{johnsonChargeTransferFine1972a,
  title = {Charge {{Transfer}} and {{Fine Structure Transitions}} in {{Kr}}\$\^+(\^{{2P}}\_j)\$+{{Kr}} and {{Xe}}\$\^+(\^{{2P}}\_j)\$+{{Xe Collisions}}},
