@article{brinkmannPlasmaSheathTransition2011a,
  title = {The Plasma\textendash Sheath Transition in Low Temperature Plasmas: On the Existence of a Collisionally Modified {{Bohm}} Criterion},
  author = {Brinkmann, R. P.},
  year = {2011},
  journal = {J. Phys. D: Appl. Phys.},
  volume = {44},
  pages = {042002},
  doi = {10.1088/0022-3727/44/4/042002},
  abstract = {The plasma\textendash sheath transition in stationary low temperature plasmas is investigated for arbitrary levels of collisionality. The model under study contains the equations of continuity and motion for a single ion species, Boltzmann?s equilibrium for the electrons and Poisson?s equation for the ?eld. Assuming that the electron Debye length \$\textbackslash lambda\_D\$ is small compared with the ion gradient length \$l = n\_i/(\textbackslash partial n\_i/\textbackslash partial x)\$, a ?rst order differential equation is established for the ion density ni as a function of the transformed spatial coordinate \$q = \i nt n\_i dx\$. A characteristic feature of this novel sheath equation is an internal singularity of the saddle point type which separates the depletion-?eld dominated sheath part of the solution from the ambipolar diffusion-controlled plasma. The properties of this singularity allow us to de?ne, in a nonarbitrary way, a collisionally modi?ed Bohm criterion which recovers Bohm?s original expression in the collisionless limit but also remains meaningful when collisions are included. A comparison is made with the collisionally modi?ed Bohm criteria proposed by Godyak (1982 Phys. Lett. A 89 80), Valentini (1996 Phys. Plasmas 3 1459) and Chen (1997 Phys. Plasmas \textbackslash bf 5 804) as well as with the approaches of Riemann (1991 J. Phys. D: Appl. Phys. \textbackslash bf 24 493) and Franklin (2003 J. Phys. D: Appl. Phys. \textbackslash bf 36 2821), who argued that the de?nition of a collisionally de?ned Bohm criterion is not possible.}
}

