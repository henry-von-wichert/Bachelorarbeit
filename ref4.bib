  author = {Zhang, Lianhua and Shao, Jianxiong and Chen, Ximeng},
  year = {2016},
  month = jun,
  journal = {Vacuum},
  volume = {128},
  pages = {137--145},
  issn = {0042207X},
  doi = {10.1016/j.vacuum.2016.03.011},
  abstract = {The commercial computational fluid dynamics (CFD) software OpenFOAM was used to simulate the gas flow field in a direct current (DC) magnetron nanocluster source. The density of free metal atoms in the aggregation region influenced the cluster formation and the cluster intensity. In order to improve the density of nanoclusters, two factors should be considered: redeposition of the sputtered atoms within the cathode region and the loss of the nanoclusters on the chamber wall. The angles of the jet and the vortex in the aggregation region have a significant influence on the number of nanoclusters. The extent to which to the Brownian diffusion can affect the particle extraction from source is investigated. Simulations have shown that the Brownian motion perturbs the clusters from the trajectories dictated by the carrier gas and increases the rate of cluster deposition on the source internal wall.},
  langid = {english},
  file = {W\:\\Literatur_Kersten\\paper_HK\\1-s2.0-S0042207X16300549-main.pdf}
}

@article{zhangRetardingPotentialAnalyzer2016a,
  title = {A Retarding Potential Analyzer Design for {{keV-level}} Ion Thruster Beams},
  author = {Zhang, Zhe and Tang, Haibin and Zhang, Zun and Wang, Joseph and Cao, Shuai},
  year = {2016},
  journal = {Rev. Sci. Instrum.},
  volume = {87},
  pages = {123510},
  doi = {10.1063/1.4972345},
  abstract = {We present a new Retarding Potential Analyzer (RPA) design that is capable of measuring keV-level energy, high-density plasma beams. This instrument overcomes the limitations of existing RPAs and can operate in plasmas with densities in excess of \textsuperscript{15\$}\textbackslash,m\textsuperscript{-3\$} and ion energies up to 1200 eV. The RPA design parameters were determined by analyzing the electron density and temperature, the sheath thickness, and the ion density in the beam based on the Faraday probe and Langmuir probe measurements. A previously unobserved grid spacing arcing phenomenon was observed in experiments. This arcing phenomenon was also investigated and a grid spacing criterion was proposed to eliminate the arcing. We present measurement results on the plasma beam emitted from the 20 cm Xenon ion thruster used on the Chinese SJ-9A satellite.},
  keywords = {retarding potential analyzer,thruster}
}

