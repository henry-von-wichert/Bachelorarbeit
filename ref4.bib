@article{versteeghLonglivingPlasmoidsAtmospheric2008a,
  title = {Long-Living Plasmoids from an Atmospheric Water Discharge},
  author = {Versteegh, A. and Behringer, K. and Fantz, U. and Fussmann, G. and J{\"u}ttner, B. and Noack, S.},
  year = {2008},
  journal = {Plasma Sources Sci. Technol.},
  volume = {17},
  pages = {024014},
  doi = {10.1088/0963-0252/17/2/024014},
  abstract = {Ball-like plasmoids were generated from discharging a capacitor bank via a water surface. In the autonomous stage after current zero they have diameters up to 0.2 m and lifetimes of some hundreds of milliseconds, thus resembling ball lightning in some way. They were studied by applying high speed cameras, electric probes, calorimetric measurements, and spectroscopy. The plasmoids are found to consist of a true plasma surrounded by a cold envelope. Decreasing electron densities in the order of \textsuperscript{-3\$} were measured from Stark broadening in the initial (formation) phase. The electron temperature is estimated to be 2000 \textendash{} 5000 K during most of the plasmoid's lifetime. The temperature of the neutral particles can exceed 1300 K. Calcium hydroxide molecular band emission is the major source of visible radiation in the autonomous phase. Chemiluminescence reactions between dissociation products of water and dissolved calcium are proposed as a source for this emission. The plasmoid's colder boundary layer consists of electric double layers that may be attributed to the characteristic shape of the balls.},
  keywords = {Kugelblitz}
}

