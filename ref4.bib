  keywords = {APPJ,kinpen,LIF,MBMS,optical diagnostic},
  file = {W\:\\Bibliothek\\Literatur\\Artikel\\Iseni2014Nitric.pdf}
}

@article{ishiharaRotationDustParticulate2001a,
  title = {On the Rotation of a Dust Particulate in an Ion Flow in a Magnetic Field},
  author = {Ishihara, O. and Sato, N.},
  year = {2001},
  journal = {IEEE Trans. Plasma Sci.},
  volume = {29},
  pages = {179--181},
  doi = {10.1109/27.923689},
  abstract = {Spinning motion of a charged test dust particulate placed in a plasma sheath is studied. The shear of the ion flow velocity in the sheath is found to be responsible for a self-rotation of the particulate, resulting in a formation of a magnetic dipole moment. The presence of an external magnetic field will cause the spinning dust particulate to precess around the magnetic field direction, while the particulate will experience the cyclotron motion. The spinning frequency is on the order of 10 Hz, while the precession is much slower for the laboratory dust experimental conditions.}
}

