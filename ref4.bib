@article{enokidoFundamentalStudiesAcceleration2000a,
  title = {Fundamental {{Studies}} on {{Acceleration}} of {{Charged Micro Particles}} ({{Macrons}}) and {{Its Interaction}} with {{Materials Surfaces}} by {{Implantation}}},
