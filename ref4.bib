@inproceedings{trottenbergInterferometricForceProbe2015a,
  title = {An {{Interferometric Force Probe}} for {{Thruster Plume Diagnostics}}},
  booktitle = {34th {{International Electric Propulsion Conference}}, {{Kobe}}, {{Japan}}},
  author = {Trottenberg, Thomas and Spethmann, Alexander and Kersten, Holger},
  year = {2015},
  pages = {IEPC-2015-419},
  abstract = {In this article, we present a novel probe for the measurement of forces exerted by a beam of ions and neutral atoms on a small test surface. The probe is intended for thruster plume diagnostics, in particular for the determination of the spatial distribution of the momentum in an exhaust beam. The instrument makes use of a cantilever whose deflection is measured interferometrically along two axes, which enables a simultaneous measurement of two independent components of the force vector. This contribution focuses on setup and calibration of the probe. An example measurement in an ion beam is presented together with a comparison measurement done with an earlier developed force probe based on a different functional principle.}
}

