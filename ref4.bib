@article{gavrilenkoMeasurementMethodElectric2000a,
  title = {Measurement Method for Electric Fields Based on {{Stark}} Spectroscopy of Argon Atoms},
  author = {Gavrilenko, V. P. and Kim, H. J. and Ikutake, T. and Kim, J. B. and Choi, Y. W. and Bowden, M. D. and Muraoka, K.},
  year = {2000},
  journal = {Phys. Rev. E},
  volume = {62},
  pages = {7201--7208},
  doi = {10.1103/PhysRevE.62.7201},
  abstract = {We report the development of a method for the measurement of electric fields in glow discharge plasmas, based on Stark spectroscopy of argon atoms. The method is based on laser excitation of transitions in atomic argon. The key feature of the method is that the electric field is determined by matching experimentally obtained absorption spectra to theoretically calculated spectra. The dependence of the positions of energy levels of argon atoms on the strength of the electric field was calculated by solving the Schro\textasciidieresis dinger equation for the argon atom. Measurements of Stark spectra were made in the sheath region of a glow discharge using laser optogalvanic spectroscopy. The wavelength of the laser radiation was tuned to the transitions 4s ={$>$} n f (n57,8, . . . ,14) of the argon atom. For n511, the lower limit for electric field measurements was estimated to be 14 V/mm.},
  keywords = {spectroscopy}
}

@article{gellerElectronCyclotronResonance1976a,
  title = {Electron {{Cyclotron Resonance Multiply Charged Ion Sources}}},
  author = {Geller, R.},
  year = {1976},
  journal = {IEEE Trans. Nucl. Sci.},
  volume = {23},
  pages = {904--912},
  doi = {10.1109/TNS.1976.4328376},
  abstract = {We describe three ion sources working in our laboratory that deliver multiply charged ion beams. All of them are E.C.R. ion sources and are characterized by the fact that the electrons are emitted by the plasma itself and are accelerated to the adequate energy through electron cyclotron resonance (E.C.R.). They can work without interruption during several months in a quasi-continuous regime. (Duty cycle: {$<$}.5). Their charge state distributions (C.S.D.) depend on the size and the characteristics of the plasma. For the smallest source the performances are comparable to those of P.I.G. sources. For the biggest source, the C.S.D. tend towards those obtained by EBIS-type sources. Experimental results and guiding ideas concerning these three ion sources are reviewed.}
}

@incollection{gerlingBasicPrinciplesFuture2022,
  title = {Basic {{Principles}} and {{Future Developments}} in {{Cold Plasma Therapy}}},
  booktitle = {Textbook of {{Good Clinical Practice}} in {{Cold Plasma Therapy}}},
  author = {Gerling, Torsten and Bansemer, Robert and Timmermann, Eric and Weltmann, Klaus-Dieter},
  editor = {Metelmann, Hans-Robert and {von Woedtke}, Thomas and Weltmann, Klaus-Dieter and Emmert, Steffen},
  year = {2022},
  pages = {257--280},
  publisher = {{Springer International Publishing}},
  address = {{Cham}},
  doi = {10.1007/978-3-030-87857-3_15},
  abstract = {Plasma as the fourth state of matter is a dominant state in space and is also known on earth in the form of lightning or aurora borealis. In the field of plasma medicine, devices based on cold plasma discharges comparable to tiny and soft lightning are used for a variety of medical indications. This chapter explains the basics of plasma discharges and their quite long history within medical applications, albeit some particular contributions have been doubtful. Scientifically sound cold atmospheric plasma (CAP) devices for medicine are already available in different geometries and each with a unique setting of a so-called cocktail of active components. The unique properties of each device concept are explained and future perspectives for medical applications are outlined. Synergies with the development of plasma medical applications elevate other fields of plasma technology and are described as well. The chapter closes with a call for joint approaches to develop new fields of treatment.},
  isbn = {978-3-030-87857-3},
  langid = {english},
  keywords = {Biotechnology,Cold atmospheric plasma,Future innovations,Idea to prototype,Medical devices,Plasma devices,Plasma medicine}
}

@article{gesselNOProductionRF2013,
  title = {{{NO}} Production in an {{RF}} Plasma Jet at Atmospheric Pressure},
  author = {van Gessel, A. F. H. and Alards, K. M. J. and Bruggeman, P. J.},
  year = {2013},
  month = jun,
  journal = {Journal of Physics D: Applied Physics},
  volume = {46},
  number = {26},
  pages = {265202},
  publisher = {{IOP Publishing}},
  issn = {0022-3727},
  doi = {10.1088/0022-3727/46/26/265202},
  abstract = {A time modulated RF atmospheric pressure plasma jet, operated in ambient air with a flow of argon with a few per cent of air, N2 or O2, was characterized by measuring the gas temperature with Rayleigh scattering, the absolute NO density with laser-induced fluorescence, and the emission of NO A and N2 C with time resolved optical emission spectroscopy. The gas temperature, NO density and the emission measurements are carried out both time and spatially resolved. The atmospheric pressure plasma jet has the advantage that the plasma dissipated power can be measured, and it was found that the gas temperature depends on the power, rather than the gas mixture. The NO density increases with increasing plasma power, and was found to have a maximum around 1.5 \texttimes{} 1021 m-3 at an air admixture of 2\%. The N2 C emission is modulated by the 13.9 MHz RF frequency, while the NO A emission front increases with much slower velocity during the 20 kHz duty cycle, which gives an insight into the excitation mechanisms in the plasma. Through the addition of either N2 or O2 to the plasma it was experimentally confirmed that the production of atomic N radicals are of key importance for the NO production in this atmospheric pressure plasma jet.},
  langid = {english}
}

