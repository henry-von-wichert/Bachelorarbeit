@article{hollensteinSiliconOxideParticle1998,
  title = {Silicon Oxide Particle Formation in {{RF}} Plasmas Investigated by Infrared Absorption Spectroscopy and Mass Spectrometry},
  author = {Hollenstein, Ch and Howling, A A and Courteille, C and Magni, D and Scholz, S M and Kroesen, G M W and Simons, N and de Zeeuw, W and Schwarzenbach, W},
