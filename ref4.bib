@article{kewitzInfluenceNozzleHead2021,
  title = {Influence of the Nozzle Head Geometry on the Energy Flux of an Atmospheric Pressure Plasma Jet},
  author = {Kewitz, Thorben and Regula, Christoph and Fr{\"o}hlich, Maik and Ihde, J{\"o}rg and Kersten, Holger},
