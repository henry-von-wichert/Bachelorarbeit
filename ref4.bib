@article{vaughanNewFormulaSecondary1989a,
  title = {A {{New Formula}} for {{Secondary Emission Yield}}},
  author = {Vaughan, J. R. M.},
  year = {1989},
  journal = {IEEE Trans. Electron Devices},
  pages = {1963--1967},
  doi = {10.1109/16.34278},
  abstract = {Mathematical expressions for the secondary emission yield as a function of impact voltage and direction are required by computer programs for ray tracing, which are also intended to follow secondary trajectories. It is shown that the accuracy requirements are quite severe for lower voltage impacts, up to about three times Vmax (where Vmax is the voltage at which the normal secondary emission ratio reaches its maximum value, \textbackslash delta\_max), but are less stringent above this range. A new formula for the low-voltage region, which agrees with experimental data better than the formula of R.G. Lye and A.J. Dekker (1957), is proposed. For the voltage region above 4 Vmax, the Lye and Dekker formula is retained, with a smooth transition centered between 3 and 4 Vmax. The directional effect is modeled by adjusting the values of \textbackslash delta\_max and Vmax before applying either formula, rather than applying either Bruining's or Muller's correction factor afterward. This results in an automatic modeling of the known absence of any appreciable directional effect below about half Vmax. Some experimental verification of the formula used to modify \textbackslash delta\_max is presented}
}

