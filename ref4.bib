@article{hansenStepEdgeSputtering2004,
  title = {Step {{Edge Sputtering Yield}} at {{Grazing Incidence Ion Bombardment}}},
  author = {Hansen, Henri and Polop, Celia and Michely, Thomas and Friedrich, Andreas and Urbassek, Herbert M.},
