@article{pielThermalStructuresResonance1985,
  title = {Thermal Structures on Resonance Cones in a Weakly Collisional Plasma},
  author = {Piel, A. and Oelerich, G.},
  year = {1985},
  journal = {Phys. Fluids},
  volume = {28},
  pages = {1366--1370},
  doi = {10.1063/1.864969},
  abstract = {Resonance cones in the range of the lower-hybrid frequency are investigated experimentally and theoretically. Two competing models, which alternatively relate the cone structure to the curvature of the dispersion branch or to the onset of damping are considered. In an extended model the superposition of both effects is studied by numerical integration of the potential. The cone structures related to the second effect turn out to be sensitive to collisional damping.}
}

@article{pierceLimitingStableCurrent1944a,
  title = {Limiting {{Stable Current}} in {{Electron Beams}} in the {{Presence}} of {{Ions}}},
  author = {Pierce, J. R.},
  year = {1944},
  journal = {J. Appl. Phys.},
  volume = {15},
  pages = {721--726},
  doi = {10.1063/1.1707378},
  abstract = {In electron flow, the net electronic charge may be neutralized by positive ions. In this case, for a given geometry there is a limiting current beyond which homogeneous flow is unstable. This limiting current is evaluated for flow normal to parallel plane equipotentials and for flow filling a conducting tube and constrained to motion parallel to the axis. For parallel planes at a potential V0 volts and spaced a distance L cm apart, the limiting current density in amperes/cm\$\^2\$ is \$i\_0 = 10\^4 \textbackslash times 10\^-6 V\_0\^\textbackslash frac32 / L\^2\$. For a long conducting tube the limiting current is \$I\_0 = 160 \textbackslash times 10\^-6 V\_0\^\textbackslash frac32\$. These limiting currents are roughly 6 times as great as in the absence of ions.}
}

